<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>Index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddEntry</servlet-name>
        <jsp-file>/addentry.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddEntry</servlet-name>
        <url-pattern>/addentry</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RmEntry</servlet-name>
        <jsp-file>/rmentry.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>RmEntry</servlet-name>
        <url-pattern>/rmentry</url-pattern>
    </servlet-mapping>
    <security-constraint>
        <web-resource-collection>
            <url-pattern>/addentry</url-pattern>
            <url-pattern>/addentry.jsp</url-pattern>
            <url-pattern>/rmentry</url-pattern>
            <url-pattern>/rmentry.jsp</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>*</role-name>
        </auth-constraint>
    </security-constraint>
</web-app>
